override_dh_auto_configure:
	echo `dh_auto_configure --no-act -- PREFIX=/opt/mountainview |sed -e 's/-nocache//'`
	eval `dh_auto_configure --no-act -- PREFIX=/opt/mountainview |sed -e 's/-nocache//'`
	

